{
  "compilerOptions": {
    "strict": true,
    "noImplicitAny": true,
    "strictNullChecks": true,
    "strictPropertyInitialization": false,
    "esModuleInterop": true,
    "moduleResolution": "node",
    "target": "ES2020",
    "module": "commonjs",
    "resolveJsonModule": true,
    "skipLibCheck": true
  }
}